package com.ericsson.fmt.forecasting.utils;


/**
 * 
 * @author esutdal
 *
 */
public class ForecastPersistenceException extends Exception{

	/**
	 * 
	 */
	private static final long serialVersionUID = -5721974427211404377L;
	
	public ForecastPersistenceException()
    {
		super();
    }

    public ForecastPersistenceException(String message)
    {
        super(message);
    }

    public ForecastPersistenceException(String message, Throwable cause)
    {
        super(message, cause);
    }

    public ForecastPersistenceException(Throwable cause)
    {
        super(cause);
    }

}
