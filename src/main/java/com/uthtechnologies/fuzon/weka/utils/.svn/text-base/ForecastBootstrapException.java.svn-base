package com.ericsson.fmt.forecasting.utils;


public class ForecastBootstrapException extends RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = -4990677239448389558L;
	
	public ForecastBootstrapException()
    {
		super();
    }

    public ForecastBootstrapException(String message)
    {
        super(message);
    }

    public ForecastBootstrapException(String message, Throwable cause)
    {
        super(message, cause);
    }

    public ForecastBootstrapException(Throwable cause)
    {
        super(cause);
    }

}
